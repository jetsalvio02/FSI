{
  "expo": {
    "name": "TrashTag",
    "scheme": "TrashTag",
    "slug": "TrashTag",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-white-bg.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/plain-white.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-removebg-preview.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo-removebg-preview.png"
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
